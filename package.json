    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250109.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0",
    "wrangler": "^4.59.1"
  },
  "keywords": [
    "mcp",
    "claude",
    "skillport",
    "plugin-marketplace",
    "cloudflare-workers"
