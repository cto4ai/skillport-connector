{
  "name": "skillport-connector",
  "version": "0.1.0",
  "description": "MCP Connector bridging Plugin Marketplaces to Claude.ai and Claude Desktop",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "dev:authless": "wrangler dev -c wrangler-authless.toml",
